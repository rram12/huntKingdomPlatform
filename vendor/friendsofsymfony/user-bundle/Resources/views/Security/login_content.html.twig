                    <form action="{{ path("fos_user_security_check") }}" method="post">
                        {% if csrf_token %}
                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                        {% endif %}
                    <div class="login_wrapper">
                        <div class="row">
                        </div>
                        <div class="formsix-pos">
                            <div class="form-group i-email"> <!--*-**USERNAME****-->
                                <input type="text" id="username" name="_username" class="form-control" placeholder="Username*"  value="{{ last_username }}" required="required" autocomplete="username" />
                            </div>
                        </div>
                        <div class="formsix-e">
                            <div class="form-group i-password"><!--*-**PASSWORD****-->
                                <input type="password" id="password" class="form-control" name="_password" placeholder="Password *" required="required" autocomplete="current-password" />
                            </div>
                        </div>
                        <div class="login_remember_box"><!--*-**REMEMBER****-->
                            <label class="control control--checkbox">{{'Remember me'|trans }}
                                <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                                <span class="control__indicator"></span>
                            </label>
                            <a href="{{ asset('#')}}" class="forget_password">
                                Forgot Password
                            </a>
                        </div>
                        <div class="login_btn_wrapper">
                            <input type="submit" id="_submit" class="btn1" name="_submit" value="{{ 'Login'|trans }}" />
                        </div>
                        <div class="login_message">
                            <p>Donâ€™t have an account ? <a href="{{ path('fos_user_registration_register') }}"> Register Now </a> </p>
                        </div>
                    </div>
                    </form>
