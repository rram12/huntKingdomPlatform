<div class="register_section">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="section-heading">
                    <h2>new customer register here</h2>
                    <p>Register got your account with weapon store is free,quick and easy.</p>
                </div>
            </div>
            <div class="col-md-8 col-md-offset-2">
                {% trans_default_domain 'FOSUserBundle' %}

                {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register','novalidate': 'novalidate','id':'formRegistration'}}) }}

                <div class="register_wrapper_box">
                    <div class="register_left_form">

                        <div class="jp_regiter_top_heading">
                            <p>Fields with * are mandetory </p>
                        </div>
                        <div class="row clearfix">


                            <b> First Name</b>{{ form_widget(form.firstName, { 'id': 'firstName'} ) }}
                            <div class="alert alert-danger" id="errorfirstName" hidden>
                                Enter First name
                            </div>

                            <b> Last Name</b>{{ form_widget(form.lastName, { 'id': 'lastName'} ) }}
                            <div class="alert alert-danger" id="errorlastName" hidden>
                                Enter Last name
                            </div>

                            <b> Address</b>{{ form_widget(form.address, { 'id': 'address'} ) }}
                            <div class="alert alert-danger" id="erroraddress" hidden>
                                Enter Address
                            </div>

                            <b> Phone Number</b>{{ form_widget(form.phoneNumber, { 'id': 'phoneNumber'} ) }}
                            <div class="alert alert-danger" id="errorphoneNumber" hidden>
                                Enter Phone Number
                            </div>

                            <b> Gender</b>{{ form_widget(form.gender, { 'id': 'gender'} ) }}


                            <b> Picture</b>{{ form_widget(form.picture, { 'id': 'picture'} ) }}
                            <div class="alert alert-danger" id="errorpicture" hidden>
                                Choose Picture
                            </div>

                            <b> Type</b>{{ form_widget(form.roles, { 'id': 'roles'} ) }}
                            <div class="alert alert-danger" id="errorroles" hidden>
                                Choose Type
                            </div>


                             <div hidden id="licence">
                            <b> Licence</b>{{ form_widget(form.contractFile, { 'id': 'contractFile'} ) }}
                            <div class="alert alert-danger" id="errorcontractFile" hidden>
                                Enter Licence
                            </div>
                             </div>
                            <b> Email</b>{{ form_widget(form.email, { 'id': 'email'} ) }}
                            <div class="alert alert-danger" id="erroremail" hidden>
                                Enter Email
                            </div>

                            <b> Username</b>{{ form_widget(form.username, { 'id': 'username'} ) }}
                            <div class="alert alert-danger" id="errorusername" hidden>
                                Enter Username
                            </div>

                            {{ form_widget(form.plainPassword, { 'id': 'plainPassword'} ) }}
                            <div class="alert alert-danger" id="errorplainPassword" hidden>
                                Enter Password
                            </div>










                            <div class="form-group col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="check-box text-center">
                                    <input type="checkbox" name="shipping-option" id="account-option_2" required="required"> &ensp;
                                    <!-- Button trigger modal -->
                                    <label for="account-option_2" class="label_2">I agreed to the <button type="button" class="check_box_anchr" data-toggle="modal" data-target="#basicExampleModal">Terms and Conditions</button>governing the use of weapon store</label>

                                </div>
                            </div>
                        </div>

                        <div class="login_btn_wrapper register_btn_wrapper login_wrapper register_wrapper_btn">
                            <input type="submit" value="{{ 'Register'|trans }}" class="btn1" />
                        </div>
                        {{ form_end(form) }}
                        <div class="login_message">
                            <p>Already a member? <a href="{{ path('fos_user_security_login')}}"> Login Here </a> </p>
                        </div>

                    </div>

                </div>
                <p class="btm_txt_register_form">In case you are using a public/shared computer we recommend that you logout to prevent any un-authorized access to your account</p>
            </div>
        </div>
    </div>
</div>

